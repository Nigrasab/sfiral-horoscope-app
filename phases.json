{
  "0-5":   {"title": "0–5 лет",   "text": "Зачатие импульса. Формирование основы телесности.",                      "mantra": "Прохожу зарождение. Формирую основу."},
  "5-10":  {"title": "5–10 лет",  "text": "Формирование восприятия. Впитывание мира.",                             "mantra": "Открываюсь опыту. Впитываю чистоту."},
  "10-15": {"title": "10–15 лет", "text": "Индивидуализация. Сборка контуров Я.",                                   "mantra": "Выстраиваю границы. Осознаю форму."},
  "15-20": {"title": "15–20 лет", "text": "Рост воли. Поиск ориентации.",                                            "mantra": "Концентрирую силу. Указываю направление."},
  "20-25": {"title": "20–25 лет", "text": "Консолидация личного центра.",                                           "mantra": "Собираю ядро. Укрепляю точку опоры."},
  "25-30": {"title": "25–30 лет", "text": "Экспансия. Выход в реализацию.",                                           "mantra": "Раскрываюсь действием. Проявляю замысел."},
  "30-35": {"title": "30–35 лет", "text": "Стабилизация и пересмотр намерений.",                                     "mantra": "Слежу за курсом. Корректирую путь."},
  "35-40": {"title": "35–40 лет", "text": "Финал витка. Предельная форма.",                                           "mantra": "Достигаю границы. Завершаю расширение."},
  "40-45": {"title": "40–45 лет", "text": "Вход в S‑петлю. Снятие внешнего импульса.",                               "mantra": "Отпускаю напряжение. Сворачиваю вглубь."},
  "45-50": {"title": "45–50 лет", "text": "Дуга. Смещение ориентации внутрь.",                                        "mantra": "Переношу взгляд внутрь. Слышу оттенки."},
  "50-55": {"title": "50–55 лет", "text": "Фаза разрыва. Переформатирование цели.",                                   "mantra": "Отпускаю прежние формы. Слышу новый зов. Становлюсь собой заново."},
  "55-60": {"title": "55–60 лет", "text": "Антидуга. Сбор пережитого в новый порядок.",                               "mantra": "Собираю опыт. Структурирую суть."},
  "60-65": {"title": "60–65 лет", "text": "Выход из петли. Вхождение в антивиток.",                                   "mantra": "Разворачиваюсь назад. Нахожу внутренний след."},
  "65-70": {"title": "65–70 лет", "text": "Обратное движение. Осознание прожитого.",                                  "mantra": "Созерцаю отрезки пути. Осмысляю переживания."},
  "70-75": {"title": "70–75 лет", "text": "Переоценка и сжатие смысла.",                                               "mantra": "Сжимаю форму. Оставляю главное."},
  "75-80": {"title": "75–80 лет", "text": "Углубление и отпускание.",                                                  "mantra": "Отпускаю лишнее. Углубляюсь в тишину."},
  "80-85": {"title": "80–85 лет", "text": "Прозрачность и равновесие.",                                                "mantra": "Становлюсь прозрачным. Сохраняю баланс."},
  "85-90": {"title": "85–90 лет", "text": "Подготовка к завершению цикла.",                                            "mantra": "Суммирую опыт. Готовлю переход."},
  "90-95": {"title": "90–95 лет", "text": "Сжатие. Переход в тонкие слои.",                                            "mantra": "Сгущаю присутствие. Выхожу за форму."},
  "95-100": {"title": "95–100 лет", "text": "Завершение антивитка. Снятие фигуры.",                                   "mantra": "Снимаю остаточное. Замыкаю окружность."},
  "100-105": {"title": "100–105 лет", "text": "Пунктир. Порог внешнего.",                                              "mantra": "Перехожу порог. Легко касаюсь мира."},
  "105-110": {"title": "105–110 лет", "text": "Разрешение остаточного импульса.",                                    "mantra": "Рассеиваю след. Растворяю различия."},
  "110-115": {"title": "110–115 лет", "text": "Слияние с фоном. Растворение.",                                         "mantra": "Сливаюсь с фоном. Исчезаю."},
  "115-120": {"title": "115–120 лет", "text": "Исчезновение различий. Покой полного разворота.",                      "mantra": "Покой. Разворот завершён."}
}
